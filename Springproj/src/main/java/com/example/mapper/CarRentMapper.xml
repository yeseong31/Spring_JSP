<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="car_rent.mybatis.CarRentMapper">
    <insert id="insert" parameterType="CarRentDTO">
        insert into car_rent values(default, #{acceptance_date}, #{return_date}, default, #{price}, #{userid}, #{car_id})
    </insert>
    <select id="getMemberByUserid" parameterType="String" resultType="CarRentDTO">
        select * from car_rent where userid=#{userid}
    </select>
</mapper>